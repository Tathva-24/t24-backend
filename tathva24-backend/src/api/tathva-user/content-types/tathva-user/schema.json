{
  "kind": "collectionType",
  "collectionName": "tathva_users",
  "info": {
    "singularName": "tathva-user",
    "pluralName": "tathva-users",
    "displayName": "TathvaUser"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "maxLength": 64,
      "minLength": 2
    },
    "Email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "Institute": {
      "type": "string",
      "required": false
    },
    "refCode": {
      "type": "string",
      "minLength": 6,
      "maxLength": 6
    },
    "state": {
      "type": "string"
    },
    "district": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male,",
        "Female,",
        "Other"
      ]
    },
    "TathvaID": {
      "type": "uid",
      "required": true
    }
  }
}
